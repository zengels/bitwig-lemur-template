canvas_fillRect(c,w/8*i,h*.45,w/64-offset,h);
}" trigger_script="" trigger_type="255" trigger_name="onRedraw" trigger="1" clock="0" clock_div="4" osc_message="/Mixer/Mix_group/Vu_background/draw" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
